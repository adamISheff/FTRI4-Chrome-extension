{
  "manifest_version": 3,
  "name": "Background Image Generator",
  "version": "1.1",
  "description": "Get an Unsplashed background slideshow while you search...",
  "author" : "",
  "homepage_url": "http://google.com",
  "icons": { 
    "16": "./imgs/icon16.png",
    "48": "./imgs/icon48.png",
    "128": "./imgs/icon128.png" 
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ]
}
